{
  "name": "vue-instant",
  "description": "vue instant allows you to easily create custom search controls with auto suggestions for your vue 2 application.",
  "version": "1.0.4",
  "author": {
    "name": "santiago",
    "email": "santiago.blanco.vilchez@gmail.com"
  },
  "keywords": [
    "vue",
    "vuejs",
    "plugin",
    "autocomplete",
    "autosuggest",
    "instant search",
    "instant",
    "searchbox",
    "component"
  ],
  "license": "MIT",
  "main": "dist/vue-instant.common.js",
  "unpkg": "dist/vue-instant.browser.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --config config/webpack.config.dev.js --progress --watch",
    "build": "npm run build:browser && npm run build:common",
    "build:browser": "cross-env NODE_ENV=production webpack --config config/webpack.config.browser.js --progress --hide-modules",
    "build:common": "cross-env NODE_ENV=production webpack --config config/webpack.config.common.js --progress --hide-modules",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/santiblanko/vue-instant.git"
  },
  "bugs": {
    "url": "https://github.com/santiblanko/vue-instant/issues"
  },
  "homepage": "https://github.com/santiblanko/vue-instant#readme",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^1.6.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.6",
    "webpack": "^2.1.0-beta.28",
    "webpack-merge": "^1.1.2"
  },
  "dependencies": {
    "vue": "^2.1.8",
    "vue-clickaway": "2.1.0"
  }
}
